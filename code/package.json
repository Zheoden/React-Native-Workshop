{
  "name": "RNWorkshop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage=true",
    "test:e2e:android": "cd ./src/__tests__/e2e && yarn android:app && cd ../../../",
    "test:e2e:ios": "cd ./src/__tests__/e2e && yarn ios:app && cd ../../../",
    "lint": "eslint \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/masked-view": "^0.1.6",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/react-redux": "^7.1.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "i18next": "^19.1.0",
    "jest-enzyme": "^7.1.2",
    "jsdom": "^16.2.2",
    "react": "16.9.0",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.3.1",
    "react-native": "0.61.5",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-localize": "^1.3.3",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-screens": "^2.0.0-alpha.29",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^2.0.16",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.60.25",
    "@types/react-test-renderer": "16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.9.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "jest-junit": {
    "suiteName": "React Native Workshop Unit Tests",
    "outputDirectory": "test-results",
    "outputName": "unit-tests.xml"
  }
}
